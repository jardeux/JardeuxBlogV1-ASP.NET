<body>
<!-- START PAGE SOURCE -->
<div class="main">
  <div class="header">
    <div class="header_resize">
      <div class="menu_nav">
        <ul>
          <li class="active"><a asp-controller="Blogs" asp-action="Index">Ana sayfa</a></li>
          <li><a asp-action="Support">Destek</a></li>
